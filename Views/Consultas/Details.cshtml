@model Trabalho.Models.Consulta

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detalhes</h1>

<div>
    <h4>Consulta</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.paciente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.paciente.endereco)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.medico)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.medico.especialidade)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Medicamento_Injetaveis)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Medicamento_Injetaveis.nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Qtde_Vacina)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Qtde_Vacina)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model?.id">Editar</a> |
    <a asp-action="Index" class="btn btn-secondary">Voltar</a>
</div>
