@model IEnumerable<Trabalho.Models.Agrupamento.ConsultaAgr>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Agrupamento</h1>

<table class="table table-bordered">
    <thead>
        <tr style="background-color: #ADD8E6; color: #000;">
            <th>
                Médico
            </th>
            <th>
                Especialidade
            </th>
            <th>
                Paciente
            </th>
            <th>
                Medicamento
            </th>
            <th>
                Quantidade Utilizada
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nomeMed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.especialidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nomePa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nomeMedicamento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtdeUsada)
            </td>
        </tr>
}
    </tbody>
</table>
